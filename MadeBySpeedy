local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

local s = Instance.new("ScreenGui")
s.Parent = playerGui

local url = "https://raw.githubusercontent.com/TokyozxcSpedy/JKK/refs/heads/main/noFilter__2_-removebg-preview-removebg-preview.png"
local fileName = "NoFilter-icon.png"

-- Download image if it doesn't exist
if not isfile(fileName) then
    writefile(fileName, game:HttpGet(url))
end

-- Create GUI for image
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = playerGui

local imageLabel = Instance.new("ImageLabel")
imageLabel.Size = UDim2.new(0.10, -20, 0.22, -20)
imageLabel.Position = UDim2.new(0.45, 15, -0.20, 0) -- Start off-screen (top)
imageLabel.Image = getcustomasset(fileName)
imageLabel.BackgroundTransparency = 1
imageLabel.ImageTransparency = 1 -- Start fully transparent
imageLabel.Parent = screenGui

-- Create GUI for text
local textGui = Instance.new("ScreenGui")
textGui.Parent = playerGui

local textLabel = Instance.new("TextLabel")
textLabel.Parent = textGui
textLabel.Position = UDim2.new(0.5, -200, 0.74, -50)
textLabel.Size = UDim2.new(0, 400, 0, 50)
textLabel.TextSize = 27
textLabel.TextColor3 = Color3.fromRGB(0, 0, 0) -- Text color
textLabel.BackgroundTransparency = 1
textLabel.Text = "" -- Initially empty
textLabel.TextStrokeTransparency = 0
textLabel.TextStrokeColor3 = Color3.fromRGB(255, 0, 0) -- Stroke color

-- Apply the custom font
textLabel.FontFace = Font.new(
    "rbxasset://fonts/families/SourceSansPro.json",
    Enum.FontWeight.Bold,
    Enum.FontStyle.Normal
)

-- Easing function (ease-out cubic)
local function easeOutCubic(t)
    return 1 - math.pow(1 - t, 3)
end

-- Typing effect
local function typeText(text)
    local index = 1
    while index <= #text do
        textLabel.Text = string.sub(text, 1, index)
        index = index + 1
        wait(0.02)
    end
end

-- Color transition for the text
local function colorTransition(label, startColor, endColor, duration)
    for i = 0, 1, 1 / (duration * 30) do
        local r = startColor.R + (endColor.R - startColor.R) * i
        local g = startColor.G + (endColor.G - startColor.G) * i
        local b = startColor.B + (endColor.B - startColor.B) * i
        label.TextColor3 = Color3.new(r, g, b)
        wait(1 / 30) -- Wait for next frame
    end
    label.TextColor3 = endColor -- Ensure final color
end

-- Fade-out effects for both text and image
local function fadeOut()
    for i = 1, 20 do
        textLabel.TextTransparency = i / 20
        imageLabel.ImageTransparency = imageLabel.ImageTransparency + 0.05
        textLabel.TextStrokeTransparency = textLabel.TextStrokeTransparency + 0.05
        wait(0.05)
    end
end

-- Image fade-in and move effect
local duration = 1.2 -- Duration of the image animation in seconds
local startTime = tick()
local startY = 0.20
local endY = 0.48

-- Start the text typing animation and color transition immediately
coroutine.wrap(function()
    typeText("Made By Speedy Credits To Cacalat") -- Text content
    wait(1)
    -- Color transition from white to red
    colorTransition(textLabel, Color3.fromRGB(255, 255, 255), Color3.fromRGB(255, 0, 0), 1)
    fadeOut()
end)()

-- Image animation
while tick() - startTime < duration do
    local elapsed = tick() - startTime
    local alpha = elapsed / duration
    local easedAlpha = easeOutCubic(alpha)

    -- Update position and transparency
    local newY = startY + (endY - startY) * easedAlpha
    imageLabel.Position = UDim2.new(0.45, 15, newY, 0)
    imageLabel.ImageTransparency = 1 - easedAlpha

    wait()
end

-- Ensure final position and transparency are correct
imageLabel.Position = UDim2.new(0.45, 15, 0.48, 0)
imageLabel.ImageTransparency = 0

-- Player respawn handling
local function onCharacterAdded()
    screenGui:Destroy()
    textGui:Destroy()
end

player.CharacterAdded:Connect(onCharacterAdded)

local LocalPlayer = game.Players.LocalPlayer

local function typewriterEffect(label, text, delayTime)
    label.Text = ""
    for i = 1, #text do
        label.Text = label.Text .. text:sub(i, i)
        wait(delayTime)
    end
end

local function resetHotbarText()
    coroutine.wrap(function()
        local magicHealth = LocalPlayer.PlayerGui.ScreenGui:WaitForChild("MagicHealth")
        local healthBar = magicHealth:WaitForChild("Health"):WaitForChild("Bar"):WaitForChild("Bar")
        local healthGlow = magicHealth.Health.Glow
        local hotbar = LocalPlayer.PlayerGui.Hotbar.Backpack.Hotbar

        -- Image for the ult Bar
        healthBar.Image = "rbxassetid://15248647876"
        healthGlow.Image = "rbxassetid://15248647876"

       healthBar.ImageColor3 = Color3.fromRGB(22, 14, 240)
        game:GetService("TweenService"):Create(healthBar, TweenInfo.new(1.5, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1, true), {ImageColor3 = Color3.fromRGB(139, 0, 0)}):Play()

       
        magicHealth.TextLabel.Text = "Credit stuff here."

        typewriterEffect(hotbar["1"].Base.ToolName, "1", 0.03)
        typewriterEffect(hotbar["The Punches"].Base.ToolName, "2", 0.03)
        typewriterEffect(hotbar["Shover"].Base.ToolName, "3", 0.03)
        typewriterEffect(hotbar["UPPERCUT"].Base.ToolName, "4", 0.03)

        -- font and scale for tool names
        for i = 1, 4 do
            local toolNameLabel = hotbar[tostring(i)].Base.ToolName
            toolNameLabel.Font = Enum.Font.Antique
        end

       
        typewriterEffect(magicHealth.TextLabel, "?", 0.05)
    end)()
end

resetHotbarText()

